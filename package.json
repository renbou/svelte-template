{
  "name": "svelte-template",
  "version": "0.1.0",
  "description": "Typical Svelte template.",
  "scripts": {
    "dev": "vite",
    "compile": "cross-env NODE_ENV=production vite build",
    "check": "svelte-check --human",
    "build:svelteconfig": "esbuild svelte.config.ts --outfile=svelte.config.js",
    "watch:svelte": "svelte-check --human --watch",
    "start": "run-p watch:* dev",
    "build": "run-s check compile",
    "serve": "sirv dist --single"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renbou/svelte-template.git"
  },
  "type": "module",
  "author": "renbou",
  "license": "Apache-2.0",
  "homepage": "https://github.com/renbou/svelte-template#readme",
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.41",
    "@tsconfig/svelte": "^3.0.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.32",
    "npm-run-all": "^4.1.5",
    "sirv-cli": "^2.0.2",
    "svelte": "^3.46.6",
    "svelte-check": "^2.4.6",
    "svelte-preprocess": "^4.10.5",
    "typescript": "^4.6.3",
    "vite": "^2.9.1"
  }
}
